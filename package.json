{
  "name": "wtfr-bot",
  "version": "1.7.0",
  "description": "Bot pseudo WTFR",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "deploy": "pnpm run build && node dist/deploy-commands.js",
    "deploy:guild": "DEPLOY_SCOPE=guild pnpm run deploy",
    "deploy:global": "DEPLOY_SCOPE=global pnpm run deploy",
    "go": "pnpm run build && pnpm run start",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:dbpush": "prisma db push",
    "bump-version": "ts-node scripts/bumpVersion.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "canvas": "^3.2.1",
    "cookie-parser": "^1.4.7",
    "discord.js": "^14.13.0",
    "dotenv": "^16.4.5",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "moment": "^2.30.1",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "pnpm": "^10.30.0"
  },
  "optionalDependencies": {
    "@discordjs/rest": "^1.0.2",
    "discord-api-types": "^0.37.35"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.4.2",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.15",
    "@types/passport-oauth2": "^1.8.0",
    "@types/serve-static": "^2.2.0",
    "prisma": "^7.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.6"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
